log
@FC defined in .configure supersedes default
@
text
@#$MLSF95.$MLSPLAT -- a file full of macros to be included by a Makefile
#to compile a Fortran 9x program or library

# "$Id: Abs.Linux,v 1.2 2007/05/31 18:29:50 pwagner Exp $"
ifndef USEMYFC
FC = f95
endif

DUSTY = $(FOPTS)

GC_IN_LDOPTS := $(shell echo ${LDOPTS} | grep '\-gc')
ifeq ($(GC_IN_LDOPTS),)
BAD_MATCH=-gc
else
BAD_MATCH=no -gc
endif

# How to prefix search paths for include and .mod files
PATH_OPTS=--set -d -prefix=-p --set -d -prefixn=-I - 

