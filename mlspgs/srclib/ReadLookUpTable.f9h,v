! $Id: ReadLookUpTable.f9h,v 1.1 2007/03/02 18:17:57 pwagner Exp $

! -------------------------------------------------  ReadLookUpTable  -----

  ! This family of routines Reads a table from a file where it is stored
  ! Something like this (although without the "!")
  ! Celsius2Fahrenheit.txt
  ! file of Celsius Fahrenheit conversions
  ! C        F
  ! -40.   -40.
  !  0.     32.
  ! 1.e2   212
  !
  ! subroutine ReadLookUpTable_r4 ( filename, N, x, y )
  !  integer, parameter :: RK = R4
  !   ! Formal arguments
  character(len=*), intent(in)                :: filename
  real(rk), dimension(:), intent(out)         :: x, y
  integer, intent(out)                        :: N ! How many pts in table
  ! Internal variables
  real(rk) :: x1, y1
  integer :: lun
  integer :: nSize
  integer :: status
  ! Executable
  nSize = min( size(x), size(y) )
  N = 0
  if ( NSize < 1 ) return
  open( newunit=lun, form='formatted', &
    & file=trim(Filename), status='old', iostat=status )
  if ( status /= 0 ) return
  do
    read( lun, fmt=*, err=100, end=900 ) x1, y1
    N = N + 1
    if ( n > NSize ) exit
    x(N) = x1
    y(N) = y1
100 continue
  enddo
900 close( UNIT=lun, iostat=status )
  return
  ! end subroutine ReadLookUpTable_r4
! $Log: ReadLookUpTable.f9h,v $
@
