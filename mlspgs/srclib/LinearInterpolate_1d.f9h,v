! $Id: LinearInterpolate_1d.f9h,v 1.2 2009/10/03 00:37:30 vsnyder Exp $

! function LinearInterpolate_1d_r4 ( values, coords ) result(value)
    ! Args
    real(rk), dimension(:), intent(in)   :: values
    real(rk), dimension(1), intent(in)   :: coords
    real(rk), dimension(2), intent(in)   :: vertices
    real(rk)                             :: value
    ! Internal variables
    real(rk) :: eps
    ! Executable
    eps = ( coords(1) - vertices(1) ) / ( vertices(2) - vertices(1) )
    value = (1._rk - eps) * values(1) + eps*values(2)
! end function LinearInterpolate_1d_r*

! $Log: LinearInterpolate_1d.f9h,v $
@
