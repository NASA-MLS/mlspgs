! export authority as may be required before exporting such information to
! foreign countries or providing access to foreign persons.

! subroutine Solve_Factored_Symm_Tri_Xa_r* ( H, D, U, Z )

    ! Solve the system A b = L U b = z, where A has been factored as L U,
    ! and its factors are represented by H, D, and U.  The solution
    ! is returned in Z.  Solve with multiple RHS vectors.

    real(rk), intent(in) :: H(:)  ! (1:n-1) Subdiagonal of L
    real(rk), intent(in) :: D(:)  ! (1:n) Inverse of diagonal of L
    real(rk), intent(in) :: U(:)  ! (1:n-2) Superdiagonal of U
                                  ! Diagonal of U is 1
    real(rk), intent(inout) :: Z(:,:) ! (1:n) input: RHS, output: solution

    integer :: K, N

    n = size(d)

    ! Solve L y = z.  Use Z temporarily for y
    z(1,:) = z(1,:) * d(1)
    do k = 2, n
      z(k,:) = ( z(k,:) - h(k-1) * z(k-1,:) ) * d(k)
    end do

    ! Solve U b = y, returning b in Z
    do k = n-1, 1, -1
      z(k,:) = z(k,:) - u(k) * z(k+1,:)
    end do

! end subroutine Solve_Factored_Symm_Tri_Xa_r*

! $Id: $

! $Log: $
@
