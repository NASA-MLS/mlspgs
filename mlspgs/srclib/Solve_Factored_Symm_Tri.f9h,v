! subroutine Solve_Factored_Symm_Tri_r* ( H, D, U, Z, B )

    ! Solve the system A b = L U b = z, where A has been factored as L U,
    ! and its factors are represented by H, D, and U.

    real(rk), intent(in) :: H(:)  ! (1:n-1) Subdiagonal of L
    real(rk), intent(in) :: D(:)  ! (1:n) Inverse of diagonal of L
    real(rk), intent(in) :: U(:)  ! (1:n-2) Superdiagonal of U
                                  ! Diagonal of U is 1
    real(rk), intent(in) :: Z(:)  ! (1:n) RHS
    real(rk), intent(out) :: B(:) ! (1:n) Solution, temporary solution of L y = z

    integer :: K, N

    n = size(d)

    ! Solve L b = z
    b(1) = z(1) * d(1)
    do k = 2, n
      b(k) = ( z(k) - h(k-1) * b(k-1) ) * d(k)
    end do

    ! Solve U b = b
    do k = n-1, 1, -1
      b(k) = b(k) - u(k) * b(k+1)
    end do

! end subroutine Solve_Factored_Symm_Tri_r*

! $Id: Solve_Factored_Symm_Tri.f9h,v 1.1 2009/11/18 00:07:11 vsnyder Exp $

! $Log: Solve_Factored_Symm_Tri.f9h,v $
! Revision 1.1  2009/11/18 00:07:11  vsnyder
! Bodies for Symm_Tri
!
@


1.1
log
@Bodies for Symm_Tri
@
text
@d22 1
a22 3
    real(rk), intent(out) :: B(:) ! (1:n) Solution

    real(rk) :: Y(size(d)-1)
d28 4
a31 4
    ! Solve L y = z
    y(1) = z(1) * d(1)
    do k = 2, n-1
      y(k) = ( z(k) - h(k-1) * y(k-1) ) * d(k)
d34 1
a34 2
    ! Solve U b = y
    b(n) = ( z(n) - h(n-1) * y(n-1) ) * d(n) ! y(n), if we had computed it
d36 1
a36 1
      b(k) = y(k) - u(k) * b(k+1)
d41 1
a41 1
! $Id: $
d43 4
a46 1
! $Log: $
@

