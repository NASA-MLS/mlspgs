! foreign countries or providing access to foreign persons.

! subroutine Solve_Factored_Symm_Tri_a ( H, D, U, Z, B )

    ! Solve the system A b = L U b = z, where A has been factored as L U,
    ! and its factors are represented by H, D, and U.  Solve with
    ! multiple RHS vectors.

    real(rk), intent(in) :: H(:)  ! (1:n-1) Subdiagonal of L
    real(rk), intent(in) :: D(:)  ! (1:n) Inverse of diagonal of L
    real(rk), intent(in) :: U(:)  ! (1:n-2) Superdiagonal of U
                                  ! Diagonal of U is 1
    real(rk), intent(in) :: Z(:,:)  ! (1:n) RHS
    real(rk), intent(out) :: B(:,:) ! (1:n) Solution

    real(rk) :: Y(size(d)-1)

    integer :: K, L, N

    n = size(d)

    do l = 1, size(b,2)
      ! Solve L y = z
      y(1) = z(1,l) * d(1)
      do k = 2, n-1
        y(k) = ( z(k,l) - h(k-1) * y(k-1) ) * d(k)
      end do

      ! Solve U b = y
      b(n,l) = ( z(n,l) - h(n-1) * y(n-1) ) * d(n) ! y(n), if we had computed it
      do k = n-1, 1, -1
        b(k,l) = y(k) - u(k) * b(k+1,l)
      end do
    end do

! end subroutine Solve_Factored_Symm_Tri_a

! $Id: $

! $Log: $
@
