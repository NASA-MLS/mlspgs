    integer :: MyLow(2), STATUS
    call deallocate_Test ( To_Allocate, ItsName, ModuleName )
    myLow = 1
    if ( present(low) ) myLow = low
    allocate ( To_Allocate(myLow(1):dim(1),myLow(2):dim(2)), stat=status )
    call test_allocate ( status, moduleName, ItsName, myLow, dim, &
      & eSize * len(to_allocate) )
    if ( status == 0 ) then
      if ( present(fill) ) then
        to_allocate = fill
      else if ( clearOnAllocate ) then
        to_allocate = default
      end if
    end if
    if ( trackallocates > 0 ) call trackAllocate ( to_allocate, itsName, moduleName )

