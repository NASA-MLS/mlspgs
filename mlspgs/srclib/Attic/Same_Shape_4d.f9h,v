! subroutine Same_Shape_*_4d ( Ref, New, ItsName, ModuleName )
!   Some_Type, pointer, dimension(:) :: Ref, New
    character(*), intent(in) :: ItsName, ModuleName
    if ( .not. associated(ref) ) then
      call deallocate_test ( new, itsName, moduleName )
    else if ( .not. associated(new) ) then
      call allocate_test ( new, &
        & ubound(ref,1), ubound(ref,2), ubound(ref,3), ubound(ref,4), &
        & itsName, moduleName, &
        & lbound(ref,1), lbound(ref,2), lbound(ref,3), lbound(ref,4) )
    else
      if ( any( shape(ref) /= shape(new) ) ) &
        & call allocate_test ( new, &
          & ubound(ref,1), ubound(ref,2), ubound(ref,3), ubound(ref,4), &
          & itsName, moduleName, &
          & lbound(ref,1), lbound(ref,2), lbound(ref,3), lbound(ref,4) )
    end if
! end subroutine Same_Shape_*_4d

