1.46
log
@Comments to subtly urge user to switch to newmlsl2path.sh
@
text
@#! /bin/bash
# Copyright 2005, by the California Institute of Technology. ALL
# RIGHTS RESERVED. United States Government Sponsorship acknowledged. Any
# commercial use must be negotiated with the Office of Technology Transfer
# at the California Institute of Technology.

# This software may be subject to U.S. export control laws. By accepting this
# software, the user agrees to comply with all applicable U.S. export laws and
# regulations. User has the responsibility to obtain export licenses, or other
# export authority as may be required before exporting such information to
# foreign countries or providing access to foreign persons.

# $Id: getmlsl2path.sh,v 1.45 2005/01/18 05:23:20 livesey Exp $
# This shell script prints the name of the desired directory 
# for a given file type supplied as an arg

# Used when expanding l2cf files
# see also newmlsl2path.sh and setupcalfiles.l2cf

# This script represents the old way
# First work out where we are.
if [ -f ~/machine.id ]
then
  MACHINE=$(cat ~/machine.id)
else
  MACHINE="desktop"
fi
SUMS=0

VERSION=v1.0.1
SIMULATION=s5
YEAR=1996
DOY=''
FWMVERSION=''
L1BVERSION=''
L2PCNAME=''
MYUSER=$USER

# Now work out what we have been asked for.
while [ -n "$(echo $1)" ]; do
    # First the 'global' arguments
    if [ -z "${1##--user=*}" ]; then
	MYUSER=${1#--user=}
    fi
    if [ -z "${1##--version=*}" ]; then
	VERSION=${1#--version=}
    fi
    if [ -z "${1##--simulation=*}" ]; then
	SIMULATION=${1#--simulation=}
    fi
    if [ -z "${1##--year=*}" ]; then
	YEAR=${1#--year=}
    fi
    if [ -z "${1##--doy=*}" ]; then
	DOY=${1#--doy=}
    fi
    if [ -z "${1##--fwmVersion=*}" ]; then
	FWMVERSION=${1#--fwmVersion=}
    fi
    if [ -z "${1##--l1bVersion=*}" ]; then
	L1BVERSION=${1#--l1bVersion=}
    fi
    if [ -z "${1##--l2pcName=*}" ]; then
	L2PCNAME=${1#--l2pcName=}
    fi
    if [ $1 == "--dao" ] || [ $1 == "--gmao" ]; then
	case $MACHINE in
	    soundbarrier | scramjet )
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		if [ $YEAR == "1996" ]; then
		    echo "/data/gmao/tsyn3d_mis_p/geos4/$YEAR"
		else
		    echo "/data/gmao/D4FAPMIS/$YEAR"
		fi
	esac
    fi
    if [ $1 == "--output" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		echo "/testing/workspace/$MYUSER/$VERSION"
	esac
    fi
    if [ $1 == "--l2pc" ]; then
	case $MACHINE in
	    soundbarrier )
		echo "/data1/$MYUSER/$L2PCNAME/";;
	    scramjet )
		echo "/state/partition1/livesey";;
	    lightspeed )
		echo "/science/l2cal";;
	    desktop )
		echo "/data/emls/l2cal/$L2PCNAME";;
	esac
    fi
    if [ $1 == "--truthl2gp" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		echo "/testing/emls/l2gp/$SIMULATION--t/$YEAR";;
	esac
    fi
    if [ $1 == "--corel2gp" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
	    echo "/testing/workspace/$MYUSER/$VERSION";;
	esac
    fi
    if [ $1 == "--l1boa" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		echo "/data/emls/l1b/$L1BVERSION/$YEAR/$DOY";;
	esac
    fi
    if [ $1 == "--l1brad" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		echo "/data/emls/l1b/$L1BVERSION/$YEAR/$DOY";;
	esac
    fi
    if [ $1 == "--sidsoa" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		echo "/testing/emls/l1boa/$SIMULATION--t/$YEAR";;
	esac
    fi
    if [ $1 == "--sidsrad" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		echo "/testing/emls/l1brad/$SIMULATION/$FWMVERSION/";;
	esac
    fi
    if [ $1 == "--l1bvsp" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		echo "/testing/workspace/perun";;
	esac
    fi
    if [ $1 == "--l2cal" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		echo "/data/emls/l2cal";;
	esac
    fi
    if [ $1 == "--leapsec" ]; then
	echo "$HOME/emls/$VERSION"
    fi
    if [ $1 == "--tmp" ]; then
	case $MACHINE in
	    soundbarrier | scramjet)
		echo "/data1/$MYUSER";;
	    lightspeed )
		echo "$HOME/$VERSION";;
	    desktop )
		echo "/testing/workspace/$MYUSER/$VERSION";;
	esac
    fi
#     if [ $1 == "--sidsrad" ]; then
# 	case $MACHINE in
# 	    soundbarrier | scramjet)
# 		echo "/data1/$MYUSER";;
# 	    lightspeed )
# 		echo "$HOME/$VERSION";;
# 	    desktop )
# 		echo "/testing/workspace/$MYUSER/$VERSION";;
# 	esac
#     fi
    shift
done

# $Log: getmlsl2path.sh,v $
