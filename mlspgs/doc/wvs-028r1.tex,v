\parindent 0pt \parskip 3pt
\vspace{-21pt}

\newlength\foo
\settoheight{\foo}{\begin{equation*}\sum_{i=1}^{N_f}\end{equation*}}

\begin{longtable}{p{0.325in}|p{2.90in}|p{2.87in}}
&\hfil Frequency averaged \hfill &\hfil Monochromatic \hfill \\
\hline
&&\\[-26pt]
    & % frequency averaged
     \begin{equation*}
      I^s_c = \sum_{n=1}^{N_f} \phi_{nc} \Delta\nu_{nc}
            \sum_{i=1}^{N_p} \Delta B_{in} \tau^s_{in}
     \end{equation*}
    & % monochromatic
     \begin{equation*}
      I^s_c = \sum_{i=1}^{N_p} \Delta B_{ic} \tau^s_{ic}
     \end{equation*}
\\[-26pt]
\multirow{1}{0.3in}[-0.45in]{LBL}
    & % frequency averaged
     \begin{equation*}
      \frac{\partial I^s_n}{\partial x_k} = 
       \sum_{i=1}^{N_p} \left( \frac{\partial \Delta B_{in}}{\partial x_k}
        - \Delta B_{in}
         \sum_{j=1}^i \frac{\partial \delta^s_{jn}}{\partial x_k} \right)
         \tau^s_{in}
     \end{equation*}
    & % monochromatic
    \begin{equation*}
    \frac{\partial I^s_c}{\partial x_k} =
     \sum_{i=1}^{N_p} \left( \frac{\partial \Delta B_{ic}}{\partial x_k}
        - \Delta B_{ic}
         \sum_{j=1}^i \frac{\partial \delta^s_{jn}}{\partial x_k} \right)
         \tau^s_{ic}
     \end{equation*}
\\[-26pt]
    & % frequency averaged
     \begin{equation*}
      \frac{\partial I^s_c}{\partial x_k} =
      \frac{\partial I^s_c}{\partial x_k} =
       \sum_{n=1}^{N_f} \phi_{nc} \Delta\nu_{nc}
       \frac{\partial I^s_n}{\partial x_k}
     \end{equation*}
    & % monochromatic
     \begin{equation*}
      \rule{0mm}{\foo}
      \frac{\partial I^s_c}{\partial x_k} =
      \frac{\partial I^s_c}{\partial x_k}
     \end{equation*}
\\[-10pt]
\hline
&&\\[-26pt]
\multirow{2}{0.3in}[-60pt]{PFA}
    & \begin{equation*}
      I_c = \sum_{i=1}^{N_p} \Delta B_{ic} \tau^w_{ic}
     \end{equation*}
    & \multirow{2}{2.87in}[-60pt]{\hfil Prohibited \hfill}
\\[-10pt]
    & \begin{equation*}
    \frac{\partial I_c}{\partial x_k} =
     \sum_{i=1}^{N_p} \left( \frac{\partial \Delta B_{ic}}{\partial x_k}
        - \Delta B_{ic}
         \sum_{j=1}^i \frac{\partial \delta^w_{jc}}{\partial x_k} \right)
         \tau^w_{ic}
     \end{equation*}
\\[-5pt]
\hline
&&\\[-26pt]
    & % frequency averaged
      \begin{equation*}
      \overline{\delta I^s_{ic}} = 
       \sum_{n=1}^{N_f} \phi_{nc} \Delta\nu_{nc} \Delta B_{in} \tau^s_{in}
      \end{equation*}
    &
\\[-30pt]
\multirow{2}{0.3in}[-36pt]{LBL and PFA}
    &
      \begin{equation*}
       I_{ic} = \overline{\delta I^s_{ic}} \tau^w_{ic}
      \end{equation*}
    & \multirow{2}{2.87in}[-46pt]{Prohibited}
\\[-30pt]
    & \begin{equation*}
      I_c = \sum_{i=1}^{N_p} I_{ic}
      \end{equation*}
\\[-30pt]
    & \begin{equation*}
      \frac{\partial I_c}{\partial x_k} =
      \frac{\partial I^s_c}{\partial x_k}
       - \sum_{i=1}^{N_p} I_{ic}
         \sum_{j=1}^i \frac{\partial \delta^w_{jc}}{\partial x_k}
     \end{equation*}
\\
\hline
Notes&\multicolumn{2}{p{6in}}
 {$N_f$ is the number of points in the pointing frequency grid.
  $n$ is an index in the pointing frequency grid.
  $N_p$ is the number of points in the line-of-sight path.
  $i$ is an index in the line-of-sight path.
  $c$ is a channel index.  $s$ indicates a strong-line (LBL) result.
  $w$ indicates a weak-line (PFA) result.}
\end{longtable}

\newpage

Denote the incremental radiance contribution at the $i^{\text{th}}$ point along
the line-of-sight path by $\delta I^\sigma_{iq} = \Delta B_{iq}
\tau^\sigma_{iq}$ for $\sigma$ either $s$ or $w$ and $q$ either $c$ or $n$. 
Then

\begin{longtable}{p{0.325in}|p{2.90in}|p{2.87in}}
&\hfil Frequency averaged \hfill &\hfil Monochromatic \hfill \\
\hline
&&\\[-26pt]
    & % frequency averaged
     \begin{equation*}
      I^s_c = \sum_{n=1}^{N_f} \phi_{nc} \Delta\nu_{nc}
            \sum_{i=1}^{N_p} \delta I^s_{in}
     \end{equation*}
    & % monochromatic
     \begin{equation*}
      I^s_c = \sum_{i=1}^{N_p} \delta I^s_{ic}
     \end{equation*}
\\[-26pt]
\multirow{1}{0.3in}[-0.45in]{LBL}
    & % frequency averaged
     \begin{equation*}
      \frac{\partial I^s_n}{\partial x_k} = 
       \sum_{i=1}^{N_p}
        \tau^s_{in} \frac{\partial \Delta B_{in}}{\partial x_k}
        - \delta I^s_{in}
         \sum_{j=1}^i \frac{\partial \delta^s_{jn}}{\partial x_k}
     \end{equation*}
    & % monochromatic
     \begin{equation*}
      \frac{\partial I^s_c}{\partial x_k} = 
       \sum_{i=1}^{N_p}
        \tau^s_{ic} \frac{\partial \Delta B_{ic}}{\partial x_k}
        - \delta I^s_{ic}
         \sum_{j=1}^i \frac{\partial \delta^s_{jc}}{\partial x_k}
     \end{equation*}
\\[-26pt]
    & % frequency averaged
     \begin{equation*}
      \frac{\partial I^s_c}{\partial x_k} =
      \frac{\partial I^s_c}{\partial x_k} =
       \sum_{n=1}^{N_f} \phi_{nc} \Delta\nu_{nc}
       \frac{\partial I^s_n}{\partial x_k}
     \end{equation*}
    & % monochromatic
     \begin{equation*}
      \rule{0mm}{\foo}
      \frac{\partial I^s_c}{\partial x_k} =
      \frac{\partial I^s_c}{\partial x_k}
     \end{equation*}
\\[-10pt]
\hline
&&\\[-26pt]
\multirow{2}{0.3in}[-60pt]{PFA}
    & \begin{equation*}
      I_c = \sum_{i=1}^{N_p} \delta I^w_{ic}
     \end{equation*}
    & \multirow{2}{2.87in}[-60pt]{Prohibited}
\\[-10pt]
    & \begin{equation*}
    \frac{\partial I_c}{\partial x_k} =
     \sum_{i=1}^{N_p}
      \tau^w_{ic} \frac{\partial \Delta B_{ic}}{\partial x_k}
        - \delta I^w_{ic}
         \sum_{j=1}^i \frac{\partial \delta^w_{jc}}{\partial x_k}
     \end{equation*}
\\[-5pt]
\hline
&&\\[-26pt]
    & % frequency averaged
      \begin{equation*}
      \overline{\delta I^s_{ic}} = 
       \sum_{n=1}^{N_f} \phi_{nc} \Delta\nu_{nc} \delta I^s_{in}
      \end{equation*}
    &
\\[-30pt]
\multirow{2}{0.3in}[-36pt]{LBL and PFA}
    &
      \begin{equation*}
       I_{ic} = \overline{\delta I^s_{ic}} \tau^w_{ic}
      \end{equation*}
    &  \multirow{2}{2.87in}[-46pt]{Prohibited}
\\[-30pt]
    & \begin{equation*}
      I_c = \sum_{i=1}^{N_p} I_{ic}
      \end{equation*}
\\[-30pt]
    & \begin{equation*}
      \frac{\partial I_c}{\partial x_k} =
      \frac{\partial I^s_c}{\partial x_k}
       - \sum_{i=1}^{N_p} I_{ic}
         \sum_{j=1}^i \frac{\partial \delta^w_{jc}}{\partial x_k}
     \end{equation*}
\\
\hline
\end{longtable}

{\large\bfseries\sffamily Program Variables:}

\begin{longtable}{llll}
$\Delta B_{in}$ is {\tt T\_Script\_LBL} &
$\Delta B_{ic}$ is {\tt T\_Script\_PFA} &
$\tau^s_{in}$ is {\tt Tau\_LBL} &
$\tau^w_{ic}$ is {\tt Tau\_PFA}
\\
$\delta I^\sigma_{iq}$ is {\tt Inc\_Rad\_Path} &
$\overline{\delta I^s_{ic}}$ is {\tt Rad\_Avg\_Path} &
$I_{ic}$ is also {\tt Rad\_Avg\_Path} & 
$\sum_{i=1}^{N_p} \delta I^\sigma_{iq}$ is {\tt RadV}
\\
$I_c$ or $I^s_c$ is {\tt Radiances} &
$\frac{\partial I^\sigma_q}{\partial x_k}$ is {\tt K\_}$x${\tt\_FRQ} &
$\frac{\partial I_c}{\partial x_k}$ is {\tt K\_}$x$
\\
\end{longtable}

