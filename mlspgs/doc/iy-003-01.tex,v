text
@\section{Gradients, Jacobians, and Hessians}


Gradient vector of $I_l: \mathbb{R}^Q \rightarrow \mathbb{R}$ is given as
\begin{eqnarray}
\nabla I_l(\vf) &=& \left[ \begin{array}{ccc}
\displaystyle \frac{\partial I_l}{\partial f_1} & \cdots & \displaystyle  \frac{\partial I_l}{\partial f_Q} 
\end{array} \right].
\label{eqn:Gradient_vector}
\end{eqnarray}
Jacobian matrix of $I: \mathbb{R}^Q \rightarrow \mathbb{R}^L$ is given as
\begin{eqnarray}
J I(\vf) &=& \left[ \begin{array}{ccc}
\displaystyle \frac{\partial I_1}{\partial f_1} & \cdots & \displaystyle  \frac{\partial I_1}{\partial f_Q} \\
\vdots & & \vdots \\
\displaystyle \frac{\partial I_L}{\partial f_1}  & \cdots & \displaystyle  \frac{\partial I_L}{\partial f_Q}
\end{array} \right].
\label{eqn:Jacobian_matrix}
\end{eqnarray}
Hessian matrix of $I_l: \mathbb{R}^Q \rightarrow \mathbb{R}$ is given as
\begin{eqnarray}
H I_l(\vf) &=& J(\nabla I_l(\vf)) \ = \ \left[ \begin{array}{c}
\displaystyle \nabla \Big( \frac{\partial I_l}{\partial f_1} \Big) \\
\vdots \\
\displaystyle \nabla \Big( \frac{\partial I_l}{\partial f_Q} \Big)
\end{array} \right] \ = \ \left[ \begin{array}{ccc}
\displaystyle \frac{\partial^2 I_l}{\partial f_1 \partial f_1} & \cdots & \displaystyle  \frac{\partial^2 I_l}{\partial f_Q \partial f_1} \\
\vdots & & \vdots \\
\displaystyle \frac{\partial^2 I_l}{\partial f_1 \partial f_Q} & \cdots & \displaystyle  \frac{\partial^2 I_l}{\partial f_Q \partial f_Q} 
\end{array} \right].
\label{eqn:Hessian_matrix}
\end{eqnarray}
Since $I$ is a function from $\mathbb{R}^Q \rightarrow \mathbb{R}^L$, i.e.
\begin{eqnarray*}
I(\vf) = (I_1, I_2, \ldots, I_L),
\end{eqnarray*}
then the array of second partial derivatives is not a two-dimensional matrix of size $Q \times Q$, but rather a tensor of order 3, with dimensions $L \times Q \times Q$.


\clearpage


\section{Sensitivity Measurements}

Denote $\vf^*$ be an unperturbed state vector, for which the radiance $I_l(\vf^*)$ at level $L$, as well as gradient vector $\nabla I_l(\vf^*)$ and Hessian matrix $HI_l(\vf^*)$ had been calculated.  Let $\vf$ denote the perturbed state vector, with perturbation defined as $\triangle \vf = \vf - \vf^*$. \\
\ \\
Accuracy of an $n-th$ order Taylor expansion can be assessed via calculating $I_l(\vf)$ analytically, and comparing the result with an approximation $I_l^{approx}(\vf)$ obtained using the Taylor expansion.


\subsection{First-Order Model}

The accuracy of the first order Taylor expansion can be determined by comparing how similar $I_l(\vf)$ is to the result obtained with the first order Taylor polynomial:
\begin{eqnarray*}
I_l(\vf) \approx I_l(\vf^*) + \sum_{q=1}^{Q} \frac{\partial I_l}{\partial f_q} (\vf^*) \cdot (f_q-f_q^*).
\end{eqnarray*}
This can be written in matrix notation:
\begin{eqnarray*}
I(\vf) \approx I(\vf^*) + J I(\vf^*) \cdot (\vf-\vf^*)
\end{eqnarray*}
or
\begin{eqnarray*}
I(\vf^* + \triangle \vf) \approx I(\vf^*) + J I(\vf^*) \cdot \triangle \vf.
\end{eqnarray*}


\subsection{Second-Order Model}

The accuracy of the second order Taylor expansion can be determined using
\begin{eqnarray*}
I_l(\vf) \approx I_l(\vf^*) + \sum_{q=1}^{Q} \frac{\partial I_l}{\partial f_q} (\vf^*) \cdot (f_q-f_q^*) + \frac{1}{2} \sum_{q,\tilde{q}=1}^{Q} \frac{\partial^2 I_l}{\partial f_q \partial f_{\tilde{q}}} (\vf^*) \cdot (f_q-f_q^*) \cdot (f_{\tilde{q}}-f_{\tilde{q}}^*),
\end{eqnarray*}
or
\begin{eqnarray*}
I_l(\vf) \approx I_l(\vf^*) + \nabla I_l(\vf^*) \cdot \triangle \vf + \frac{1}{2} (\triangle \vf)^t \cdot H I_l(\vf^*) \cdot \triangle \vf.
\end{eqnarray*}
This can not be directly reformulated in {\it full} matrix notation. However, we could generalize hessian matrix $H$ to $\hat{H}$ and write:
\begin{eqnarray*}
I(\vf) \approx I(\vf^*) + J I(\vf^*) \cdot (\vf-\vf^*) + \frac{1}{2} (\vf-\vf^*)^t \cdot \hat{H} I(\vf^*) \cdot (\vf-\vf^*).
\end{eqnarray*}


\clearpage


\subsubsection{Numerical Computations}

Denote $\triangle \vf = \vf - \vf^*$. \\
The {\bf first order} sensitivity measurement can be tested approximating $I_l(\vf)$ as:
\begin{eqnarray*}
I_l^{approx}(\vf) = I_l(\vf^*) + \nabla I_l(\vf^*) \cdot \triangle \vf,
\end{eqnarray*}
for $l = 1, \ldots, L$. \\
The {\bf second order} sensitivity measurement can be tested with
\begin{eqnarray*}
I_l^{approx}(\vf) = I_l(\vf^*) + \nabla I_l(\vf^*) \cdot \triangle \vf + \frac{1}{2} (\triangle \vf)^t \cdot H I_l(\vf^*) \cdot \triangle \vf.
\end{eqnarray*}
\ \\
In the experiments below, $\nabla I_l(\vf^*)$ was obtained {\it analytically} for both mixing ratio and temperature state vectors, in both first-order and second-order cases.  For mixing ratios, $HI_l(\vf^{O_3 *})$ was obtained either {\it analytically} or {\it numerically}.  For temperature, $HI_l(\vf^{T *})$ was obtained {\it numerically}.



