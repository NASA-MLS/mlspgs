  subroutine Init_Vocabulary_Names
  ! Initialize VAL for names that are not defined in
  ! the grammar.  Most of these are nonterminals.
    use Symbol_Table, only: Enter_Terminal
    text(1) = enter_terminal("'->'",0)
    text(2) = enter_terminal("'='",0)
    text(3) = enter_terminal("'=>'",0)
    text(4) = enter_terminal("'?'",0)
    text(5) = enter_terminal("EOG",0)
    text(6) = enter_terminal("EOS",0)
    text(7) = enter_terminal("_SOG",0)
    text(8) = enter_terminal("name",0)
    text(9) = enter_terminal("number",0)
    text(10) = enter_terminal("string",0)
    text(11) = enter_terminal("_<GOAL>",0)
    text(12) = enter_terminal("_act",0)
    text(13) = enter_terminal("_action",0)
    text(14) = enter_terminal("_grammar",0)
    text(15) = enter_terminal("_item",0)
    text(16) = enter_terminal("_lhs",0)
    text(17) = enter_terminal("_production",0)
    text(18) = enter_terminal("_rhs",0)
    text(19) = enter_terminal("_rhs_alone",0)
    text(20) = enter_terminal("_rhss",0)
    text(21) = enter_terminal("_thing",0)
    text(22) = enter_terminal("_things",0)
    text(23) = enter_terminal("_vocab",0)
  end subroutine Init_Vocabulary_Names
