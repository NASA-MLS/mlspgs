text
@module S_STAT_TEMP_M
  use MLSCommon, only: R4, R8
  use PHYSICS, only: H_OVER_K
  implicit NONE
  private
  public :: S_STAT_TEMP, STAT_TEMP
  interface STAT_TEMP; module procedure S_STAT_TEMP; end interface

!{ This module calculates the energy emitted at the input frequency(MHz)
! from a black body source with temperature TEMP kelvins. The Returned
! energy is the statistical temperature equivalent in kelvins:
!
! \begin{equation*}
!  B = \frac{h \nu}{k \left( \exp(\frac{h \nu}{k T}) -1 \right )}
!  \end{equation*}

!---------------------------- RCS Ident Info -------------------------------
  CHARACTER (LEN=256) :: Id = &
       "$Id: s_stat_temp_m.f90,v 1.1 2000/05/04 18:12:06 vsnyder Exp $"
  CHARACTER (LEN=*), PARAMETER :: ModuleName= "$RCSfile: s_stat_temp_m.f90,v $"
!---------------------------------------------------------------------------

  integer, private, parameter :: RK = r4

contains
  pure function S_STAT_TEMP ( TEMP, FREQ ) result ( STAT_TEMP )
    include 'stat_temp.f9h'
  End function S_STAT_TEMP
end module S_STAT_TEMP_M

! $Log: s_stat_temp_m.f90,v $
! Revision 1.1  2000/05/04 18:12:06  vsnyder
! Initial conversion to Fortran 90
!
@


