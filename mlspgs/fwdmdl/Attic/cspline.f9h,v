! Copyright (c) 1999, California Institute of Technology.  ALL RIGHTS RESERVED.
! U.S. Government Sponsorship under NASA Contract NAS7-1407 is acknowledged.

! subroutine ?_CSPLINE ( XIN, XOUT, YIN, YOUT, NIN, NOUT )
d10 1
d16 1
a16 1
    real(rk) :: X, XL, XH, YL, YH, YLIN, YSPL, DYDX, DP, H
d55 2
a56 2
      yspl = yl + h * (coeff(2,klo) + h * (coeff(3,klo) +     &
   &         h * coeff(4,klo) / 3.0) / 2.0)
d58 7
a64 2
      h = max(abs(ylin),abs(yspl))
      dp = 100.0*abs(ylin-yspl)/(h+epsilon(ylin))
d66 1
a66 8
! If the spline is more than 25% different from linear, revert to
! linear interpolation
!
      if (dp < 25.0) then            ! spline ok
        yout(i) = yspl
      else                           ! revert to linear
        yout(i) = ylin
      end if
@


