! $Id: $
d6 2
a7 2
! function CS_ZeroFix_* ( W )
    real(rk)    :: R(2)
d11 5
a15 6
    equivalence (Z,R(1))

    z = w
    if ( abs(r(1)) < dinky ) r(1) = 0.0_rk
    if ( abs(r(2)) < dinky ) r(2) = 0.0_rk
    v = z
d18 4
a21 1
! $Log: $
