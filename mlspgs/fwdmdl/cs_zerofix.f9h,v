! $Id: cs_zerofix.f9h,v 2.4 2005/06/22 18:14:01 pwagner Exp $

! function CS_ZeroFix_* ( W ) result ( V )
    real(rk)    :: R, I
    real(rk), parameter :: Dinky = epsilon(r) ** 2

    r = real(w)
    i = aimag(w)
    if ( abs(r) < dinky ) r = 0.0_rk
    if ( abs(i) < dinky ) i = 0.0_rk
    v = cmplx(r,i,kind=rk)
! end function CS_ZeroFix_*

