  Subroutine GET_DRAD ( DDER, T_SCRIPT, TAU, DT_SCRIPT_DX, IndxR, IndxL, &
 &                      ILO, IHI, RDRAD )
d20 3
a22 3
    real(r4), intent(in) :: DDER(*), T_SCRIPT(*), TAU(*), DT_SCRIPT_DX(*)
