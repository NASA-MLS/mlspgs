1.8
log
@Not sure why, but this stopped repeated compiles after updates
@
text
@# "$Id: targets,v 1.7 2009/07/17 20:27:32 pwagner Exp $"

# Path to source files:
S = ..

# Path to root directory of l2, lib, etc.:
CONFDIR=../..

# include files %.f9h shared by l2, lib are in $(srclib)
srclib = $(CONFDIR)/srclib

# Set paths for finding source files (different from .o and .mod)

# This is the machines directory--its files are compiled directly into libdir
dir1 =

# This is the srclib directory--its files are compiled directly into libdir
dir2 =

# This is the blas directory
dir3 =

MODPATHS = 

UTILDIR=$(CONFDIR)/util

# reecho.sh can filter out globbed file expansions that fail
# e.g., '*.f90 *.f' -> '*.f90" if there aren't any .f files
# However we will be using it just to prefix any unprefixed preset paths
REECHO=$(UTILDIR)/reecho.sh

# Set paths for finding source files (different from .o and .mod)

vpath %.o ./
vpath %.f90 ../
vpath %.f9h ../ $(srclib)
vpath %.f ../ $(PGSINC)
vpath %.c ../

include $(srclib)/$(MLSF95).$(MLSPLAT)
# All the paths to supply .mod and INCLUDE files used in compiling %.f90
# Precede each path by "-I "
INC_PATHS := $(EXTRA_PATHS) \
  $(shell ${REECHO} ${PATH_OPTS} \
  $(UNPREFXDPTHS) $(MODPATHS) $(PGSINC) $(srclib) $S)

# Define some suffix rules

.SUFFIXES:
.SUFFIXES: .o .dbg.o .f90 .f .c
.f90.o:
	$(FC) -c $(FOPTS) $(INC_PATHS) $< $(FAFTER)

.f90.dbg.o:
	$(FC) -c $(FOPTS) $(INC_PATHS) $< -o $*.dbg.o $(FAFTER)

.f.o:
	$(FC) -c $(FOPTS) $(INC_PATHS) $< $(FAFTER)

.c.o:
	$(CC) -c $(COPTS) ${INC_PATHS} -DNAGf90Fortran $<

libmlspack.a: sdot.o

# This is the default name of the configure file
# It may be overridden for multiple platforms, special options, etc.
ifdef HOSTMLSCFILE
   MLSCFILE=$(HOSTMLSCFILE)
else
   MLSCFILE=.configure
endif

CONFDIR=../..
UTILDIR=../../util

# reecho.sh can filter out globbed file expansions that fail
# e.g., '*.f90 *.f' -> '*.f90" if there aren't any .f files
# However we will be using it just to prefix any unprefixed preset paths
REECHO=$(UTILDIR)/reecho.sh

include Makefile.dep

record_file=newAifBdiff.out

MARKING_ARG=-f$(MARK_ALL_AS_UPTODATE) -s$(SKIP_PREREQ_DIRS)

# ========================================================================
# An automatically constructed Makefile
# This file was built by mlsconfigure
# by catenating three files:
# (1) .configure -- a file of mls settings: compiler, platform, etc.
# (2) common     -- a file of macros specific for this directory, e.g. paths
#                   these might be compiler switches, 
# (3) targets    -- a file of targets with instructions how to build them
#                    (except for .f90.o rules which are still in MLSCONFG)

# Other files referred to by means of "include" statements:
#   srclib/$(MLSF95).$(MLSPLAT) -- macros specific for the 
#         compiler.platform combo; e.g., NAG.Sun
#   Makefile.dep -- dependencies among the primary Fortran modules
#   srclib/Makefile.dep -- dependencies among the shared Fortran modules
#   srclib/targets.h -- linker lines whenever building an executable

# ========================================================================
#          The different versions of libmlspack.a
all: libmlspack.a
#
#	if BLAS blank, LIB_BLAS != f95   sdot.o ddot.o dot_external_m.o
#	if BLAS blank, LIB_BLAS = f95        dot_intrinsic.o
#	if BLAS non-blank                    dot_external_m.o

#--------------------- default name of GNU-compatible c compiler
CC=cc

ifdef MLSINSTALLDIR
   INSTALLDIR:=$(MLSINSTALLDIR)
else
ifndef INSTALLDIR
   INSTALLDIR:=$(CONFDIR)/bin/$(MLSCONFG)
endif
endif

# "yes" is safer--
# in case modules have been deleted or excluded since the last build
# Nope--will need to think harder about what to do about excluded modules
delete_lib_first = no

ifdef BLAS
   blas_message=Building mlspack without blas support
else
ifeq ($(LIB_BLAS),f95)
   blas_message=Building mlspack with blas supplied by f95 intrinsics
else
   blas_message=Building mlspack with internal blas support
endif
endif

ifdef LAPACK
   lapack_message=Building mlspack without lapack support
else
   lapack_message=Building mlspack with lapack supplied by f95 intrinsics
endif

libmlspack.a: ${OBJS} 
		@@if [ "$(MODULES)" != "" -a "$(MARK_ALL_AS_UPTODATE)" = "yes" ]; then \
		 $(UTILDIR)/mark_as_uptodate.sh -M $(MAKE) -fyes ; \
		fi
		@@echo "$(blas_message)"
		@@echo "$(lapack_message)"
ifeq ($(delete_lib_first),yes)
		rm -f $@@
endif
	ar ru $@@ $?

# ========================================================================

clean:
	rm -f *.o *.mod *.a

mark_all_as_uptodate:
		@@if [ "$(MODULES)" != "" -a "$(MARK_ALL_AS_UPTODATE)" != "no" ]; then \
		 $(UTILDIR)/mark_as_uptodate.sh -T libmlspack.a -M $(MAKE) $(MARKING_ARG); \
		fi

tar:
	rm -f libmlspack.tgz
	tar hcvzf libmlspack.tgz * --exclude \*.o --exclude \*.mod --exclude libmlspack.a

# ========================================================================

# Custom build commands for some modules

BUGGY := $(shell echo ${FOPTS} | sed 's/-O[0-9]*//')

DUSTY_NO_OPT := $(shell echo ${DUSTY} | sed 's/-O[0-9]*//')

ddot.o: ../ddot.f
	$(FC) -c $(DUSTY_NO_OPT) -O $(INC_PATHS) ../ddot.f $(FAFTER)
sdot.o: ../sdot.f
	$(FC) -c $(DUSTY_NO_OPT) -O $(INC_PATHS) ../sdot.f $(FAFTER)
sddot.o: ../sddot.f
	$(FC) -c $(DUSTY_NO_OPT) -O $(INC_PATHS) ../sddot.f $(FAFTER)

# $Log: targets,v $
# Revision 1.7  2009/07/17 20:27:32  pwagner
