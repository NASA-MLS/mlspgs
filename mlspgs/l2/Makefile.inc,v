#             -L$(TOOLKIT)/hdf/$(PLATFORM)/HDF4.1r3/lib \
#               -lmfhdf -ldf -ljpeg -lz

# ========================================================================

clean:
	rm -f *.o *.mod mlsl2

tar:
	rm -f parser.tgz
	tar hcvzf parser.tgz * --exclude \*.o --exclude \*.mod --exclude test

