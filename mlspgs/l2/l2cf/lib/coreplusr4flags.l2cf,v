  !makeQualityForOneProduct(product=ClO,chisqScale=1.0,minRads=600,cloudMax=10K,cloudMin= -10K)
  !makeStatusForOneProduct(product=ClO,chisqScale=1.0,minRads=600,cloudMax=10K,cloudMin= -10K)
