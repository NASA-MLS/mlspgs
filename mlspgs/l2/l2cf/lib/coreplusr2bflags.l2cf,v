; $Id: coreplusr2flags.l2cf,v 1.10 2004/09/21 23:02:59 livesey Exp $
d25 2
a26 2
  !define(flaggingBands,{2,3,4,5})
  !makeFlagsForOneProduct(product=CH3CN,radiometer=R2,chisqScale=1.0,minRads=4500,cloudMax=5K,cloudMin= -10K)
d38 4
