@d1 2
a2 1
; $Id: coreplusr2flags.l2cf,v 1.15 2006/10/04 16:51:47 pwagner Exp $
d4 6
d84 3
@


1.15
log
@Compatible w/ status bits from multiple radiometers; needs review
@
text
@d1 1
a1 1
; $Id: coreplusr2flags.l2cf,v 1.14 2006/08/08 22:27:07 pwagner Exp $
d11 2
a12 2
  !makeQualityForOneProduct(product=H2O,chisqScale=1.0,minRads=8300,cloudMax=15K,cloudMin= -30K)
  !makeStatusForOneProduct(product=H2O,chisqScale=1.0,minRads=8300,cloudMax=15K,cloudMin= -30K)
d17 2
a18 2
  !makeQualityForOneProduct(product=RHI,chisqScale=1.0,minRads=8300,cloudMax=15K,cloudMin= -30K)
  !makeStatusForOneProduct(product=RHI,chisqScale=1.0,minRads=8300,cloudMax=15K,cloudMin= -30K)
d23 2
a24 2
  !makeQualityForOneProduct(product=N2O,chisqScale=1.0,minRads=600,cloudMax=15K,cloudMin= -30K)
  !makeStatusForOneProduct(product=N2O,chisqScale=1.0,minRads=600,cloudMax=15K,cloudMin= -30K)
d29 2
a30 2
  !makeQualityForOneProduct(product=HNO3,chisqScale=1.0,minRads=2500,cloudMax=15K,cloudMin= -30K)
  !makeStatusForOneProduct(product=HNO3,chisqScale=1.0,minRads=2500,cloudMax=15K,cloudMin= -30K)
d35 2
a36 2
  !makeQualityForOneProduct(product=ClO,chisqScale=1.0,minRads=1200,cloudMax=15K,cloudMin= -30K)
  !makeStatusForOneProduct(product=ClO,chisqScale=1.0,minRads=1200,cloudMax=15K,cloudMin= -30K)
d41 2
a42 2
  !makeQualityForOneProduct(product=O3,chisqScale=1.0,minRads=1200,cloudMax=15K,cloudMin= -30K)
  !makeStatusForOneProduct(product=O3,chisqScale=1.0,minRads=1200,cloudMax=15K,cloudMin= -30K)
d47 2
a48 2
  !makeQualityForOneProduct(product=HCN,chisqScale=1.0,minRads=1400,cloudMax=15K,cloudMin= -30K)
  !makeStatusForOneProduct(product=HCN,chisqScale=1.0,minRads=1400,cloudMax=15K,cloudMin= -30K)
d53 2
a54 2
  !makeQualityForOneProduct(product=CH3CN,chisqScale=1.0,minRads=1200,cloudMax=15K,cloudMin= -30K)
  !makeStatusForOneProduct(product=CH3CN,chisqScale=1.0,minRads=1200,cloudMax=15K,cloudMin= -30K)
d59 2
a60 2
  !makeQualityForOneProduct(product=SO2,chisqScale=1.0,minRads=3600,cloudMax=15K,cloudMin= -30K)
  !makeStatusForOneProduct(product=SO2,chisqScale=1.0,minRads=3600,cloudMax=15K,cloudMin= -30K)
d77 3
@


