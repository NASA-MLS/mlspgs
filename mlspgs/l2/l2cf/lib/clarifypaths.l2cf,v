!ifdef(inpathl1brad,{},{!define(inpathl1brad,!inpath)})
; ------------------------------------------ Level 2 files
!ifdef(inpathtruthl2gp,{},{!define(inpathtruthl2gp,{/data/emls/l2gp/!simulationSeries--t/1996})})
!ifdef(inpathl2gp,{},{!define(inpathl2gp,!inpath)})
!ifdef(inpathl2aux,{},{!define(inpathl2aux,!inpath)})
!ifdef(outpathl2gp,{},{!define(outpathl2gp,!outpath)})
!ifdef(outpathl2aux,{},{!define(outpathl2aux,!outpath)})
; ------------------------------------------ Climatology files
!ifdef(inpathcli,{},{!define(inpathcli,!calpath)})
; ------------------------------------------ Forward model input
!ifdef(inpathfwm,{},{!define(inpathfwm,!calpath)})
; ------------------------------------------ L2PCs
!ifdef(inpathl2pc,{},{!define(inpathl2pc,!calpath)})
!ifdef(outpathl2pc,{},{!define(outpathl2pc,!outpath)})
; ------------------------------------------ Define standard filenames
; The first are l2cfs for an include, so unquoted
!define(signalsL2cf,{!calpath/MLS-Aura_L2Cal-Signals_v1-2-2_0000d000.l2cf})
!define(spectroscopyL2cf,{!calpath/MLS-Aura_L2Cal-Spectroscopy_v1-2-0_0000d000.l2cf})
