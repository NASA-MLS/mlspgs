@; This l2cf fragment clears up path definitions for the level 2 stuff.

; Go through the file types by level.
; ------------------------------------------ Calibration files
; ------------------------------------------ Edinburgh
!ifdef(flagEdinburgh,{
!ifdef(mirrorpath,{},{!define(mirrorpath,{/eosmls/jpl/data/emls})})
!ifdef(calpath,{},{!define(calpath,{!mirrorpath/l2cal})})
!ifdef(inpathtruthl2gp,{},{!define(inpathtruthl2gp,{!mirrorpath/l2gp/!simulationSeries--t/!year(!day)})})
!ifdef(inpathcli,{},{!define(inpathcli,!inpath/l2cal)})
!ifdef(climatologyFile,{},{!define(climatologyFile,{'!inpathcli/MLS-Aura_L2Cal-Climatologies_pe-feb-!phiStr-v1-0-2_0000d000.txt'})})
})

!ifdef(calpath,{},{!define(calpath,{/data/emls/l2cal})})
