a10 1
  !makeFlagsForOneProduct(product=H2O,radiometer=R2,chisqScale=1.0,minRads=6500,cloudMax=5K,cloudMin= -10K)
d14 1
a14 1
  !makeFlagsForOneProduct(product=RHI,radiometer=R2,chisqScale=1.0,minRads=6500,cloudMax=5K,cloudMin= -10K)
d18 1
a18 1
  !makeFlagsForOneProduct(product=N2O,radiometer=R2,chisqScale=1.0,minRads=1000,cloudMax=5K,cloudMin= -10K)
d30 4
a33 1
; $Log: coreplusr2flags.l2cf,v $
