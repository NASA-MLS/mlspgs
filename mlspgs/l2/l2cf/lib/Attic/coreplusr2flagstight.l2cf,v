1.3
log
@No longer needed
@
text
@; $Id: coreplusr2flagstight.l2cf,v 1.2 2004/04/30 21:40:57 livesey Exp $

; This l2cf fragment does all the quality flagging for the Core+R2
; products.


begin Fill
  ;; ------------------------------------- H2O-190
  !define(flaggingBands,{2,3,4,5,6,23})
  !makeFlagsForOneProduct(product=H2O,radiometer=R2,chisqScale=100.0,minRads=6500,cloudMax=2K,cloudMin= -10K)

  ;; --------------------------------------- N2O-190
  !define(flaggingBands,{3})
  !makeFlagsForOneProduct(product=N2O,radiometer=R2,chisqScale=50.0,minRads=1000,cloudMax=2K,cloudMin= -10K)

  ;; ---------------------------------------- HNO3-190
  !define(flaggingBands,{4,6})
  !makeFlagsForOneProduct(product=HNO3,radiometer=R2,chisqScale=10.0,minRads=2500,cloudMax=2K,cloudMin= -10K)

  ;; ----------------------------------------- ClO-190
  !define(flaggingBands,{5})
  !makeFlagsForOneProduct(product=ClO,radiometer=R2,chisqScale=10.0,minRads=1200,cloudMax=2K,cloudMin= -10K)

  ;; ----------------------------------------- O3-190
  !define(flaggingBands,{6})
  !makeFlagsForOneProduct(product=O3,radiometer=R2,chisqScale=10.0,minRads=1200,cloudMax=2K,cloudMin= -10K)

  ;; ----------------------------------------- HCN-190
  !define(flaggingBands,{6,27})
  !makeFlagsForOneProduct(product=HCN,radiometer=R2,chisqScale=10.0,minRads=1400,cloudMax=2K,cloudMin= -10K)

  ;; ----------------------------------------- CH3CN-190
  !define(flaggingBands,{2,3,4,5})
  !makeFlagsForOneProduct(product=CH3CN,radiometer=R2,chisqScale=500.0,minRads=4500,cloudMax=2K,cloudMin= -10K)

  ;; ------------------------------------- Temperature-CorePlusR2
  !define(flaggingBands,{1,22,32})
  !makeFlagsForOneProduct(product=Temperature,radiometer=R2,chisqScale=10.0,minRads=1500,cloudMax=2K,cloudMin= -10K)

  ;; ------------------------------------- GPH-CorePlusR2
  !define(flaggingBands,{1,22,32})
  !makeFlagsForOneProduct(product=GPH,radiometer=R2,chisqScale=10.0,minRads=1500,cloudMax=2K,cloudMin= -10K)

end Fill

