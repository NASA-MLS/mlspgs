text
@; $Id: coreplusr5flagstight.l2cf,v 1.1 2004/04/30 21:01:38 livesey Exp $

; This l2cf fragment does all the quality flagging for the Core+R5
; products.

begin Fill
  ;; ------------------------------------ OH-2T5
  !define(flaggingBands,{15,17,18,19})
  !makeFlagsForOneProduct(product=OH,radiometer=R5,chisqScale=20.0,minRads=2000,cloudMax=10K)

  ;; ------------------------------------ O3-2T5
  !define(flaggingBands,{15,17,18,19})
  !makeFlagsForOneProduct(product=O3,radiometer=R5,chisqScale=20.0,minRads=2000,cloudMax=10K)

  ;; ------------------------------------- Temperature-CorePlusR5
  !define(flaggingBands,{1,17,22})
  !makeFlagsForOneProduct(product=Temperature,radiometer=R2,chisqScale=10.0,minRads=800,cloudMax=10K,cloudMin= -20K)

  ;; ------------------------------------- GPH-CorePlusR5
  !define(flaggingBands,{1,17,22})
  !makeFlagsForOneProduct(product=GPH,radiometer=R2,chisqScale=10.0,minRads=800,cloudMax=10K,cloudMin= -20K)

end Fill

