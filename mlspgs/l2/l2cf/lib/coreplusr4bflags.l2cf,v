  !makeQualityForOneProduct(product=N2O,chisqScale=1.0,minRads=800,cloudMax=999K,cloudMin= -999K)
  !makeStatusForOneProduct(product=N2O,chisqScale=1.0,minRads=800,cloudMax=999K,cloudMin= -999K)
