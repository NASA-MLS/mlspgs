a19 3
  !define(flaggingRadiometers,{R1A,R1B})!dnl
  !makeQualityForOneProduct(product=N2O,chisqScale=1.0,minRads=800,cloudMax=30K,cloudMin= -4K)
  !makeStatusForOneProduct(product=N2O,chisqScale=1.0,minRads=800,cloudMax=30K,cloudMin= -4K)
d23 3
a25 3
  !define(flaggingRadiometers,{R1A,R1B})!dnl
  !makeQualityForOneProduct(product=SO2,chisqScale=1.0,minRads=800,cloudMax=30K,cloudMin= -4K)
  !makeStatusForOneProduct(product=SO2,chisqScale=1.0,minRads=800,cloudMax=30K,cloudMin= -4K)
d42 3
@


