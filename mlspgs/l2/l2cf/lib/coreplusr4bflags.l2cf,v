1.9
log
@Removed Temperature, GPH from this phase
@
text
@!divert(-1)
; $Id: coreplusr4bflags.l2cf,v 1.8 2006/11/01 23:38:52 dwu Exp $

; ----------- Phase name vs. retrieval name
; Some phase(s) may not have Retrieve sections
; and so we'll need to know the last one
!define(retrophase,{!phase})

!divert!dnl
; This l2cf fragment does all the quality flagging for the Core+R4
; products.

begin Fill

  ;; ------------------------------------ N2O-640
  !define(flaggingBands,{12})
  !define(flaggingRadiometers,{R4})!dnl
  !makeQualityForOneProduct(product=N2O,chisqScale=1.0,minRads=800,cloudMax=10K,cloudMin= -10K)
  !makeStatusForOneProduct(product=N2O,chisqScale=1.0,minRads=800,cloudMax=10K,cloudMin= -10K)

  ;; ------------------------------------ SO2-640
  !define(flaggingBands,{12})
  !define(flaggingRadiometers,{R4})!dnl
  !makeQualityForOneProduct(product=SO2,chisqScale=1.0,minRads=800,cloudMax=10K,cloudMin= -10K)
  !makeStatusForOneProduct(product=SO2,chisqScale=1.0,minRads=800,cloudMax=10K,cloudMin= -10K)

end Fill

; $Log: coreplusr4bflags.l2cf,v $
; Revision 1.8  2006/11/01 23:38:52  dwu
; undo the changes since yesterday
;
; Revision 1.7  2006/11/01 20:52:47  dwu
; compute r4 status based on R1 cloud flags
;
; Revision 1.6  2006/11/01 18:41:34  dwu
; change r4 cloud flag thresholds
;
; Revision 1.5  2006/10/11 23:04:55  pwagner
; Special retrieval phase name retrophase needed for Convergence Ratio Fill
;
; Revision 1.4  2006/10/04 16:51:47  pwagner
; Compatible w/ status bits from multiple radiometers; needs review
;
; Revision 1.3  2006/08/08 22:27:07  pwagner
; Fills Quality, Status for Temperature, GPH; needs revision before v2.20
;
; Revision 1.2  2006/07/28 18:55:49  dwu
; *** empty log message ***
;
; Revision 1.1  2006/07/28 18:50:15  dwu
; *** empty log message ***
@


1.8
log
@undo the changes since yesterday
@
text
@d2 1
a2 1
; $Id: coreplusr4bflags.l2cf,v 1.7 2006/11/01 20:52:47 dwu Exp $
a26 12
  ;; ------------------------------------- Temperature-CorePlusR4B
  !define(flaggingBands,{1})
  !define(flaggingRadiometers,{R1A,R1B})!dnl
  !makeQualityForOneProduct(product=Temperature,chisqScale=1.0,minRads=1500,cloudMax=30K,cloudMin= -4K)
  !makeStatusForOneProduct(product=Temperature,chisqScale=1.0,minRads=1500,cloudMax=30K,cloudMin= -4K)

  ;; ------------------------------------- GPH-CorePlusR4B
  !define(flaggingBands,{1})
  !define(flaggingRadiometers,{R1A,R1B})!dnl
  !makeQualityForOneProduct(product=GPH,chisqScale=1.0,minRads=1500,cloudMax=30K,cloudMin= -4K)
  !makeStatusForOneProduct(product=GPH,chisqScale=1.0,minRads=1500,cloudMax=30K,cloudMin= -4K)

d30 3
@


