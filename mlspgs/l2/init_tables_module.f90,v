! =====     Private procedures     =====================================
  ! --------------------------------------------------  ADD_IDENT  -----
  integer function ADD_IDENT ( TEXT )
    use SYMBOL_TABLE, only: ENTER_TERMINAL
    use SYMBOL_TYPES, only: T_IDENTIFIER
    character(len=*), intent(in) :: TEXT
    add_ident = enter_terminal ( text, t_identifier )
  end function ADD_IDENT

