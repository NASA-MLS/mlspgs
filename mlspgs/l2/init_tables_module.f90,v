             np+n_spec_def /) )

    call make_tree( (/ &
      begin, s+s_anyGoodValues, &
             begin, f+f_quantity, vectorQuantity(), &
             begin, f+f_precision, vectorQuantity(), &
             begin, f+f_quality, vectorQuantity(), &
