             np+n_spec_def /) )

    call make_tree( (/ &
      begin, s+s_catchwarning, &
             begin, f+f_message, string(), &
             begin, f+f_Boolean, field_spec(s_Boolean,req=req), &
             np+n_spec_def /) )

    call make_tree( (/ &
      begin, s+s_compare, &
             begin, f+f_a, vectorQuantity(), &
             begin, f+f_b, vectorQuantity(), &
