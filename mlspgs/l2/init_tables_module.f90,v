             begin, f+f_where, string(), &
             begin, f+f_resetAll, boolean(), &
             begin, f+f_reset, boolean(), ndp+n_spec_def /) )
    call make_tree ( (/ &
      begin, s+s_restrictRange, &
             begin, f+f_quantity, vectorQuantity(req), &
