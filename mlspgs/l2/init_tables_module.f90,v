    call make_tree ( (/ &
      begin, s+s_endSelect, &
             begin, f+f_label, string(), &
             ndp+n_spec_def /) )
    call make_tree ( (/ &
      begin, s+s_select, &
             begin, f+f_Boolean, field_spec(s_Boolean), &
             begin, f+f_label, string(), &
             ndp+n_spec_def /) )
    call make_tree ( (/ &
      begin, s+s_case, &
             begin, f+f_Boolean, field_spec(s_Boolean), &
