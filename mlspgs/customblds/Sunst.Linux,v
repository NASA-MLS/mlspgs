	$(UTILDIR)/newAifBdiff.sh -a ieee_arithmetic.mod $(FC) -c $(FOPTS) $(INC_PATHS) $(MACH_DIR)/ieee_arithmetic.f90 $(FAFTER)

intrinsic.o: $(S)/lit_parm.f9h $(S)/lit_add.f9h

endif

