	$(UTILDIR)/mark_as_uptodate.sh -M $(MAKE) -t -T infnan.o inf_nan_detection.mod 
inf_nan_detection.mod: infnan.f90
	$(UTILDIR)/newAifBdiff.sh -a inf_nan_detection.mod $(FC) -c $(FOPTS) $(INC_PATHS) $(MACH_DIR)/infnan.f90 $(FAFTER)

intrinsic.o: $(S)/lit_parm.f9h $(S)/lit_add.f9h

endif

