# 	${TOOLKIT_LIB}

# testvert.o: mlslib

clean:
	-rm *.o *.mod *.M

allclean: clean
	cd ../../lib ; ${MAKE} clean

include ${MLSPGS_LIB_PATH}/makelibs.inc




@


