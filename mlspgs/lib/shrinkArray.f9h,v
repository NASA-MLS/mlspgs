! $Id: shrinkArray.f9h,v 2.2 2005/06/22 17:44:11 pwagner Exp $
        ! function shrinkarray_int( array, delete, shrunken ) result (newsize)
        !   integer, dimension(:), pointer    :: array
        integer, dimension(:), intent(in) :: delete
        !   ! Local variables
        !   integer, dimension(:), pointer    :: temp => null()
        integer :: i
        integer :: newsize
        ! Executable
        newsize = min( size(array), size(shrunken) )
        if ( newsize < 1 ) return
        shrunken(1:newsize) = array(1:newsize)
        if ( size(delete) < 1 ) return
        newsize = 0
        do i=1, size(array)
          if ( any(delete == i) ) cycle
          if ( newsize >= size(shrunken) ) exit
          newsize = newsize + 1
          shrunken(newsize) = array(i)
        enddo
      ! end function shrinkArray_int
!
! $Log: shrinkArray.f9h,v $
@
