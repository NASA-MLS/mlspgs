! $Id: AGM.f9h,v 2.1 2008/06/18 20:43:01 pwagner Exp $

  ! double precision function AGM_D ( x, y ) result ( R )
  !   double precision, intent(in) :: x, y
    ! Internal variables
  !   integer                      :: i
  !   double precision             :: a0, g0, ai, gi
    eps = epsilon(x) ! We'll iterate until successive values of R differ by eps
    a0 = (x + y)/2
    g0 = sqrt(x * y)
    R = (a0 + g0)/2
    do i=1, MAXITER
      if ( abs(R-a0) <= eps ) return
      ai = (a0 + g0)/2
      gi = sqrt(a0 * g0)
      a0 = ai
      g0 = gi
      R = (a0 + g0)/2
    enddo
  ! end function AGM_D

! $Log: AGM.f9h,v $
@
