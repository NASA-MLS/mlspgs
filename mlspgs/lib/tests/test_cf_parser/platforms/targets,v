1.3
log
@Calculated dependencies of shared source lib
@
text
@include ../Makefile.dep

# "$Id: targets,v 1.2 2000/11/04 02:09:42 pwagner Exp $"

# ========================================================================
#       The objects for the shared source files (in srclib)
# Include their dependencies
include $(srclib)/Makefile.dep

#       The objects for init_table_module (in itmlib)
itmlib_objs = init_tables_module.o

# Add them to OBJS
ALLOBJS = $(OBJS) $(srclib_objs) $(itmlib_objs)

# ========================================================================
test: $(ALLOBJS) $(libdir)/libmls.a
	$(FC) -o test $(ALLOBJS) \
                      -L$(libdir) -lmls \
	-L$(PGSTK) -lPGSTK  \

out.test: test $S/in.test
	./test -a $S/in.test out.test
# ========================================================================

clean:
	rm -f *.o *.mod test out.test

tar:
	rm -f test.tgz
	tar hcvzf test.tgz * --exclude \*.o --exclude \*.mod --exclude test

# ========================================================================
# $Log: targets,v $
# Revision 1.2  2000/11/04 02:09:42  pwagner
