1.2
log
@rebuilds both tests/* and libmls.a
@
text
@#	Makefile
#	targets: configure and depends and clean
#	true targets: $(MLSCONFG)/Makefile and $(MLSCONFG)/libmls.a
#
#special case:
#the 1st time through, before .configure exists yet,
#execute $(MLSBIN)/configure to create .configure

#This file name is usu. Makefile but may be different;
#i.e., if you call are building this mls
#by calling make with a different Makefile name,
#say: make -f MakeFC
#then rename the Makefiles in /l2 and /lib also MakeFC

# "$Id: MakeFC,v 1.1 2000/10/24 20:56:46 pwagner Exp $"

MakeFName = MakeFC

# where to store .configure
CONFDIR=../..

# where configure and makemakedep.sh are stored
MLSBIN=../../util

# where the platforms directory is
PLATFORMS = .

# This will set our MLS platform, compiler. etc.
# w/o complaining if it does nor exist yet
-include $(CONFDIR)/.configure

SHELL = /bin/sh

PROG=tests

#the subdirectory where testing programs kept
#testdir = tests

#testsubdirs = multiply sparsify test_cf_parser multiply_vector cholesky
testsubdirs = multiply sparsify test_cf_parser cholesky


#================= For testing purposes only =============
alltests: $(testsubdirs)

$(PROG): $(testsubdirs)

$(testsubdirs):
	echo "Making $@@"
	$(MAKE) -f $(MakeFName) -C $@@

.PHONY: configure depends clean alltests $(testsubdirs) tar $(PROG)

tar: clean

configure:
	$(MLSBIN)/mlsconfigure -dc "$(CONFDIR)" -pc "$(PLATFORMS)" \
          -c "$(MLSF95)" -p "$(MLSPLAT)"

$(CONFDIR)/.configure:
	$(MLSBIN)/mlsconfigure -dc "$(CONFDIR)" -pc "$(PLATFORMS)" \
          -c "$(MLSF95)" -p "$(MLSPLAT)"

depends:
	@@echo "Calculating dependencies $(testsubdirs)"; \
	 for dir in $(testsubdirs); do \
	  $(MAKE) -f $(MakeFName) -C $$dir depends; \
	done

clean:
	@@echo "Cleaning $(testsubdirs)"; \
	 for dir in $(testsubdirs); do \
	  $(MAKE) -f $(MakeFName) -C $$dir clean; \
	done

tar:
	rm -f ../$(PROG).tgz
	tar hcvzf ../$(PROG).tgz * --exclude \*.o --exclude \*.mod

# $Log: MakeFC,v $
# Revision 1.1  2000/10/24 20:56:46  pwagner
# First commit
#
# Revision 2.1  2000/10/18 21:59:19  pwagner
# First commit
#

@


