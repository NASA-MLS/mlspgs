text
@#Where to find .o prerequisites that come from libmls.a
dir1 = $(libdir)/

include ../Makefile.dep

# "$Id: targets,v 1.1 2000/11/09 17:28:36 pwagner Exp $"

# ========================================================================
PROG=test_multiply_vector
$(PROG): $(OBJS) $(blas_objs) $(libdir)/libmls.a
	$(FC) -o $(PROG) $(OBJS) $(blas_objs)\
                      -L$(libdir) -lmls \
	-L$(PGSTK) -lPGSTK  \
	$(BLASLIB)  \
# ========================================================================

clean:
	rm -f *.o *.mod $(PROG) out.test

tar:
	rm -f $(PROG).tgz
	tar hcvzf $(PROG).tgz * --exclude \*.o --exclude \*.mod --exclude $(PROG)

