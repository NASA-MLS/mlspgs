! $Id: isafillvalue.f9h,v 2.9 2011/11/11 00:30:22 vsnyder Exp $
!   elemental function is_a_fill_value_int( arg ) result ( itIs )
!     integer, intent(in) :: arg
!     integer :: k
!     integer :: x
!     logical :: itIs

    if ( .not. associated(MLSFills) ) then
      itIs = ( abs(arg-int(undefinedvalue)) < 1 )
    else
      do k = 1, size(MLSFills)
        x = arg
        if ( whatCondition(MLSFills(k)%condition) > 9 ) x = abs(arg)
        select case (whatCondition(MLSFills(k)%condition))
        case (0, 10)
          itIs = equalsFillValue( x, MLSFills(k)%value )
        case (1, 11)
          itIs = ( x - MLSFills(k)%value > -0.5*MLSFills(k)%tol )
        case (2, 12)
          itIs = ( MLSFills(k)%value - x > -0.5*MLSFills(k)%tol )
        case (3, 13)
          itIs = ( x - MLSFills(k)%value >= -0.5*MLSFills(k)%tol )
        case (4, 14)
          itIs = ( MLSFills(k)%value - x >= -0.5*MLSFills(k)%tol )
        end select
        if ( itIs ) return
      enddo
    endif
!   end function is_a_fill_value_int
! $Log: isafillvalue.f9h,v $
@
