! subroutine Interpolate_1D_P_List ( Field, Eta, Path )
!   ! Interpolate Field * Eta to Path
!   real(rk), intent(in) :: Field(:)
!   type(value_1D_p_t), intent(in) :: Eta(:)
!   real(rk), intent(out) :: Path(:)

    integer :: I

    path = 0
    do i = 1, size(eta)
      path(eta(i)%p) = path(eta(i)%p) + eta(i)%v * field(eta(i)%j)
    end do

! $Log: Interpolate_1D_P.f9h,v $
