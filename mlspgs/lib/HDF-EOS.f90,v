MODULE HDFEOS               ! F90 interface to HDF-EOS.
d8 2
a9 2
   IMPLICIT NONE
   PUBLIC
d11 56
a66 29
   PRIVATE :: ID, ModuleName

!------------------- RCS Ident Info -----------------------
CHARACTER(LEN=130) :: Id = &
   "$Id: HDF-EOS.f90,v 1.4 2000/06/19 23:47:00 lungu Exp $"
CHARACTER(LEN=*), PARAMETER :: ModuleName="$RCSfile: HDF-EOS.f90,v $"
!----------------------------------------------------------



! Now define f90 interfaces for some HDF-EOS.

   INTERFACE
      INTEGER FUNCTION swopen(filename, access_mode)
        CHARACTER (LEN=*) :: filename
        INTEGER:: access_mode
      END FUNCTION swopen
!      INTEGER FUNCTION swattach(sfwid, swathname)
!         INTEGER :: swfid
!         CHARACTER (LEN=*) :: swathname
!      END FUNCTION swattach
      INTEGER FUNCTION swdetach(swid)
         INTEGER :: swid
      END FUNCTION swdetach
      INTEGER FUNCTION swclose(file_id)
        INTEGER:: file_id
      END FUNCTION swclose

   END INTERFACE
d69 1
a69 1
END MODULE HDFEOS
