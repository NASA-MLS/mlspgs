# Run the "test_multiply" test harness.

if ( .$1 == .--help ) then
  echo "Options: -s => dump the sparse representation (default false)"
  echo "         -d => dump the input matrices, and the output ones even if"
  echo "               there\'s no error (default -d)"
  echo "         -n => don't dump the matrices (default -d)"
  echo "         -q => don't ask for the input (just read it)"
  exit
endif

