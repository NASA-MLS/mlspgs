        if ( a%v(j)%n == 0 ) cycle
d30 1
a30 1
          if ( b(i)%v(k)%n == 0 ) cycle
d32 3
a34 3
          c(i)%v(l)%nf = a%v(j)%n
          c(i)%v(l)%n = b(i)%v(k)%n
          c(i)%v(l)%np = b(i)%v(k)%np
d42 3
