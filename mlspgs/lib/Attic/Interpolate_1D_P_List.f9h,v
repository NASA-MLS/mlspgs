! subroutine Interpolate_1D_P_List ( Field, Eta, Path )
!   ! Interpolate Field * Eta to Path
!   real(rk), intent(in) :: Field(:)
!   type(value_1D_p_t), intent(in) :: Eta(:)    ! Size(Eta) assumed to be
!   real(rk), intent(out) :: Path(:)            ! the same as Size(Path)

    integer :: I

    path = 0
    do i = 1, size(eta)
      path(eta(i)%p) = path(eta(i)%p) + eta(i)%v * field(eta(i)%n)
    end do

