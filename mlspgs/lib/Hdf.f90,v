  !-----------------------

  integer, parameter :: DFNT_NONE                =     0
  integer, parameter :: DFNT_QUERY               =     0
  integer, parameter :: DFNT_VERSION             =     1

  integer, parameter :: DFNT_FLOAT32             =     5
  integer, parameter :: DFNT_FLOAT               =     5
