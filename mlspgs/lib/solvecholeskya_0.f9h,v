log
@Reworded Copyright statement, moved rcs id
@
text
@! Copyright 2005, by the California Institute of Technology. ALL
! RIGHTS RESERVED. United States Government Sponsorship acknowledged. Any
! commercial use must be negotiated with the Office of Technology Transfer
! at the California Institute of Technology.

! This software may be subject to U.S. export control laws. By accepting this
! software, the user agrees to comply with all applicable U.S. export laws and
! regulations. User has the responsibility to obtain export licenses, or other
! export authority as may be required before exporting such information to
! foreign countries or providing access to foreign persons.

! $Id: solvecholeskya_0.f9h,v 2.2 2003/06/03 19:21:51 livesey Exp $

! ------------------------------------------------------------------------------
! Part of MatrixModule_0, used for r4 and r8 versions of routine of this name

    n = size(x)
    
    my_b => x
    if ( present(b) ) my_b => b
    my_t = .false.
    if ( present(transpose) ) my_t = transpose

    if ( my_t ) then ! solve U^T X = B for X
      d = u(1,1)                                                           
      if ( abs(d) < tol ) then
        if ( present ( status ) ) then
          status = 1
          return
        else
          call MLSMessage ( MLSMSG_Error, ModuleName, &    
            & "U matrix in SolveCholeskyA_0 is singular" )                   
        end if
      end if
      x(1) = my_b(1) / d                                                   
      do i = 2, n                                                          
        d = u(i,i)                                                         
        if ( abs(d) < tol ) then
          if ( present ( status ) ) then
            status = i
            return
          else
            call MLSMessage ( MLSMSG_Error, ModuleName, &  
              & "U matrix in SolveCholeskyA_0 is singular" )                 
          end if
        end if
        x(i) = ( my_b(i) - dot(i-1, u(1,i), 1, x(1), 1) ) / d              
!       x(i) = ( my_b(i) - dot_product( u(:i-1,i), x(:i-1)) ) / d          
      end do ! i = 2, n                                                    
    else             ! solve U X = B for X
      d = u(n,n)
      if ( abs(d) < tol ) then
        if ( present ( status ) ) then
          status = n
          return
        else
          call MLSMessage ( MLSMSG_Error, ModuleName, &
            & "U matrix in SolveCholeskyA_0 is singular" )
        end if
      end if
      x(n) = my_b(n) / d
      do i = n-1, 1, -1
        d = u(i,i)
        if ( abs(d) < tol ) then
          if ( present ( status ) ) then
            status = i
            return
          else
            call MLSMessage ( MLSMSG_Error, ModuleName, &
              & "U matrix in SolveCholeskyA_0 is singular" )
          end if
        end if
        x(i) = ( my_b(i) - dot(n-i, u(i,i+1), size(u,1), x(i+1), 1) ) / d
!       x(i) = ( my_b(i) - dot_product(u(i,i+1:n), x(i+1:n)) ) / d
      end do ! i = n-1, 1, -1
    end if ! my_t

    if ( present ( status ) ) status = 0

