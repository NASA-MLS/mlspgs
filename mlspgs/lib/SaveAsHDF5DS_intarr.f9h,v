    integer (HID_T) :: setID            ! ID for dataset
a30 2
    integer(hsize_t), dimension(size(shape(value))) :: SHP, MAXDIMS ! Shape
    integer :: Rank                     ! Rank of VALUE
d35 1
a35 1
    call MLSMessageCalls( 'push', constantName='SaveAsHDF5DS_intarr'//digits(rank) )
d48 1
