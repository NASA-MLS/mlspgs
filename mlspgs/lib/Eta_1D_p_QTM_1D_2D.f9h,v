! subroutine Eta_1D_p_QTM_1D_2D_* ( A, B, C )

    ! Compute the outer product C of interpolation matrices A and B,
    ! each represented as a sparse list.

!   type(value_1D_p_t(rk)), intent(in) :: A(:)
!   type(value_qtm_1d_list_t(rk)), intent(in) :: B(:)
!   type(value_qtm_2d_list_t(rk)), intent(out) :: C(:)

    ! Assume size(A) == size(B) == size(C)

    integer :: I, J, K, L

    do i = 1, size(a)
      j = a(i)%p
      l = c(j)%n
      c(j)%n = c(j)%n + b(i)%n
      do k = 1, b(i)%n
        l = l + 1
        c(j)%v(l)%jz = a(i)%j
        c(j)%v(l)%j = b(j)%v(k)%j
        c(j)%v(l)%jp = b(j)%v(k)%jp
        c(j)%v(l)%v = a(i)%v * b(j)%v(k)%v
      end do
    end do

