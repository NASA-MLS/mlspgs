! subroutine Interpolate_1D_List_* ( Field, Eta, Path )
!   ! Interpolate Field * Eta to Path
!   real(rk), intent(in) :: Field(:)
!   type(value_1D_list_t), intent(in) :: Eta(:) ! Size(Eta) assumed to be
!   real(rk), intent(out) :: Path(:)            ! the same as Size(Path)

    integer :: I, J

    do i = 1, size(eta)
      path(i) = sum( [ ( eta(i)%v(j)%v * &
                       & field(eta(i)%v(j)%j), &
                       & j = 1, eta(i)%n ) ] )
    end do

