begin GlobalSettings
    vGridTangentSids: vGrid, type=Logarithmic, coordinate=Zeta, $
       start=1000mb, formula=[37:12,24:6,3:3]
    sidsFwm: forwardModel, type=full,  $
    signals = ['R2:190.B2F:H2O.C1:23'], $
    molecules = [[O3, O3, O3_V2, O3_V1_3, O3_ASYM_O_18, O3_SYM_O_18 ], $
                 [N2O, N2O], [HNO3, HNO3],  [ SO2, S_32_O2 ], O2, N2, $
                 [H2O, H2O, H2O_R2]],$
    LSBlblMolecules = [N2, O3, O2, H2O, H2O_R2],$
    USBlblMolecules = [N2, O3, O2, H2O, H2O_R2, N2O],$
    phiWindow=0 degrees, /skipOverlaps, $
    integrationGrid=vGridTangentSids, tangentGrid=vGridTangentSids, $
    /do_conv, /do_freq_avg, $
    /allLinesForRadiometer
end GlobalSettings
