1.4
log
@deleted .f files, jonathan
@
text
@	subroutine pfsetup(n,p,dp,u,nu)
	implicit none
	
	integer i,j,n,nu
	real u(nu)
	real p(n,nu)
	real dp(n,nu)
	real w1, w2, v1, v2, us

	do 100 i=1,nu
	us = sqrt(1-u(i)*u(i))
	w1 = us
	w2 = 3*u(i)*w1
	p(1,i)=w1
	p(2,i)=w2
	v1 = u(i)
	v2 = 6*u(i)*u(i) - 3
	dp(1,i)=v1
	dp(2,i)=v2
	  do j=3,n
	  p(j,i)=(2.*j-1.)/(j-1.)*w2*u(i) - j/(j-1.)*w1
	  dp(j,i)=(2*j-1.)/(j-1.)*(u(i)*v2-w2*us)-j/(j-1.)*v1
	  w1 = w2
	  w2 = p(j,i)
	  v1 = v2
	  v2 = dp(j,i)
	  enddo
 100	continue
	return
	end

! $Log: pfsetup.f,v      
@


1.3
log
@First working version
@
text
@@


