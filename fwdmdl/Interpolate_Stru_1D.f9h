! subroutine Interpolate_Stru_1D_? ( From, Eta, To )
!   real(rk), intent(in) :: From(:)
!   type (eta_?_t), intent(in) :: Eta(:) ! Gotten from Get_Eta_Stru
!   real(rk), intent(out) :: To(:)       ! Eta and To assumed same length
    integer :: I, L
    real(rk) :: H
    do i = 1, size(to)
      l = eta(i)%l
      h = eta(i)%eta
      to(i) = from(l)*h + from(l+1)*(1.0_rk-h)
    end do
! end subroutine Interpolate_Stru_1D_?
