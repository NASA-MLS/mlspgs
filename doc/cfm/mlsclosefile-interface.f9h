!Closes an open file.
subroutine MLS_CloseFile(file, error)
   !The file to be closed.
   type(MLSFile_T) :: file
   
   !This is an optional argument; if present it will be set to:
   ! PGS_S_SUCCESS (0) when the file is closed successfully.
   ! a non-zero otherwise.
   integer, optional, intent(out) :: error
end subroutine
