! subroutine DeepCopy_* ( A, B ) ! for * > 1
!   type(...), pointer :: A(:...), B(:...)
    if ( associated(b) ) then
      call allocate_test ( a, ubound(b), 'A', moduleName, low=lbound(b) )
      a = b
    else
      call deallocate_test ( a, 'A', moduleName )
    end if
! end subroutine DeepCopy_*

