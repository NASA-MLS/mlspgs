include ../Makefile.dep

# "$Id$"

# ========================================================================
PROG=test_cholesky
$(PROG): $(OBJS) $(libdir)/libmls.a
	$(FC) -o $(PROG) $(OBJS) \
                      -L$(libdir) -lmls \
	-L$(PGSTK) -lPGSTK  \
# ========================================================================

clean:
	rm -f *.o *.mod $(PROG) out.test

tar:
	rm -f $(PROG).tgz
	tar hcvzf $(PROG).tgz * --exclude \*.o --exclude \*.mod --exclude $(PROG)

