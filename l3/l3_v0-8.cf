; -----------------------------------------------------------------------------
;
; In this last section of the l3cf, we detail how the output data
; created in the core module are sent to the various output files.
;
BEGIN Output
    ;;
    ;; Each output Level 3 product is described on a separate line, with the
    ;; name of the corresponding MCF file, and an expandable file name template
    ;; containing fields not related to granule-level identification.
    ;;
    CLOL3DM: out, mcf = "ML3DCLO.001.MCF", file="MLS-Aura_$level_ClO_"
    ZL3DM: out, mcf = "ML3DZ.001.MCF", file="MLS-Aura_$level_Z_"
    H2OL3DM: out, mcf = "ML3DH2O.001.MCF", file="MLS-Aura_$level_H2O_"
    HCLL3DM: out, mcf = "ML3DHCL.001.MCF", file="MLS-Aura_$level_HCl_"
    HCNL3DM: out, mcf = "ML3DHCN.001.MCF", file="MLS-Aura_$level_HCN_"
    HNO3L3DM: out, mcf = "ML3DHNO3.001.MCF", file="MLS-Aura_$level_HNO3_"
    N2OL3DM: out, mcf = "ML3DN2O.001.MCF", file="MLS-Aura_$level_N2O_"
    O3L3DM: out, mcf = "ML3DO3.001.MCF", file="MLS-Aura_$level_O3_"
    OHL3DM: out, mcf = "ML3DOH.001.MCF", file="MLS-Aura_$level_OH_"
    TL3DM: out, mcf = "ML3DT.001.MCF", file="MLS-Aura_$level_T_"
    ;;
END Output
